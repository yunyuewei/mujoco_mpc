    <general name="unrothum_r2_r" joint="unrothum_r2_r" gear="150" ctrllimited='true' ctrlrange='-1 1'></general> -->
    <general name="elv_angle_r" joint="elv_angle_r"  ctrllimited='true' ctrlrange='-1 1'></general>
    <general name="shoulder_elv_r" joint="shoulder_elv_r"  ctrllimited='true' ctrlrange='-1 1'></general>
    <!-- <general name="shoulder1_r2_r" joint="shoulder1_r2_r" gear="150" ctrllimited='true' ctrlrange='-1 1'></general> -->
    <general name="shoulder_rot_r" joint="shoulder_rot_r"  ctrllimited='true' ctrlrange='-1 1'></general>
    <general name="elbow_flexion_r" joint="elbow_flexion_r"  ctrllimited='true' ctrlrange='-1 1'></general>
    <general name="pro_sup_r" joint="pro_sup_r"  ctrllimited='true' ctrlrange='-1 1'></general>
    <general name="deviation_r" joint="deviation_r"  ctrllimited='true' ctrlrange='-1 1'></general>
    <general name="flexion_r" joint="flexion_r"  ctrllimited='true' ctrlrange='-1 1'></general>

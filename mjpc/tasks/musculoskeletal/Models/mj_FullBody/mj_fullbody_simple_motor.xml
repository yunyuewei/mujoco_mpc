    <general name="shoulder_elv_l" joint="shoulder_elv_l"  ctrllimited='true' ctrlrange='-1 1'></general>
    <!-- <general name="shoulder1_r2_l" joint="shoulder1_r2_l" gear="150" ctrllimited='true' ctrlrange='-1 1'></general> -->
    <general name="shoulder_rot_l" joint="shoulder_rot_l"  ctrllimited='true' ctrlrange='-1 1'></general>
    <general name="elbow_flexion_l" joint="elbow_flexion_l"  ctrllimited='true' ctrlrange='-1 1'></general>
    <general name="pro_sup_l" joint="pro_sup_l"  ctrllimited='true' ctrlrange='-1 1'></general>
